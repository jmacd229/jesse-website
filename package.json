{
    "name": "jesse-website",
    "private": true,
    "description": "A personal portfolio website for Jesse MacDougall",
    "version": "1.0.1",
    "releaseDate": "11-22-2020",
    "license": "0BSD",
    "scripts": {
        "build": "sed -i 's|\"releaseDate\": \".*\"|\"releaseDate\": \"'$(date +%m-%d-%Y)'\"|g' package.json && gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "cy:open": "cypress open",
        "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx"
    },
    "dependencies": {
        "@types/react-helmet": "^6.1.0",
        "bootstrap": "^4.6.0",
        "cypress-plugin-snapshots": "^1.4.4",
        "gatsby": "^2.31.1",
        "gatsby-image": "^2.10.0",
        "gatsby-plugin-google-gtag": "^2.7.0",
        "gatsby-plugin-manifest": "^2.11.0",
        "gatsby-plugin-purgecss": "^5.0.0",
        "gatsby-plugin-react-helmet": "^3.9.0",
        "gatsby-plugin-robots-txt": "^1.5.5",
        "gatsby-plugin-sass": "^2.8.0",
        "gatsby-plugin-sharp": "^2.13.1",
        "gatsby-plugin-transition-link": "^1.20.5",
        "gatsby-plugin-typography": "^2.11.0",
        "gatsby-plugin-webfonts": "^1.1.4",
        "gatsby-source-filesystem": "^2.10.0",
        "gatsby-transformer-sharp": "^2.11.0",
        "gsap": "^3.6.0",
        "lottie-web": "^5.7.6",
        "node-sass": "^4.14.1",
        "react": "^16.14.0",
        "react-bootstrap": "^1.4.3",
        "react-dom": "^16.14.0",
        "react-helmet": "^6.1.0",
        "react-typography": "^0.16.19",
        "rxjs": "^6.6.3",
        "sweetalert2": "^10.13.3"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "cypress": "^6.3.0",
        "cypress-plugin-tab": "^1.0.5",
        "eslint": "^7.18.0",
        "eslint-plugin-react": "^7.22.0",
        "gatsby-plugin-root-import": "^2.0.5",
        "prettier": "2.1.2",
        "start-server-and-test": "^1.11.7",
        "typescript": "^4.1.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jmacd229/jesse-website"
    },
    "bugs": {
        "url": "https://github.com/jmacd229/jesse-website/issues"
    }
}
